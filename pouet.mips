.data
crlf: .asciiz "\n"

.text
main:
	move $s7, $sp
	add $sp, $sp, -16

# Affectation de 3 à thomas
li $v0, 3          # Charge la valeur 3 dans $v0
sw $v0, 4($s7)     # Stocke la valeur de $v0 à l'adresse de thomas + 4 ($s7 + 4)

# Accès à l'index 1 du tableau thomas
li $v0, 1          # Charge l'indice 1 dans $v0
mul $a0, $v0, 4    # Multiplie l'indice par la taille d'un élément du tableau (4 bytes)
lw $a0, 4($s7)     # Charge la valeur de thomas[$v0] dans $a0

end:
	li $v0, 10
	syscall